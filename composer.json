{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.7",
        "alaouy/youtube": "^2.2",
        "alchemyguy/youtube-laravel-api": "^1.0",
        "artesaos/seotools": "^1.0",
        "cviebrock/eloquent-sluggable": "^9.0",
        "diglactic/laravel-breadcrumbs": "^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "kyslik/column-sortable": "^6.4",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "laravel/scout": "^9.6",
        "laravel/tinker": "^2.7",
        "lorisleiva/laravel-actions": "^2.4",
        "orangehill/iseed": "^3.0",
        "spatie/laravel-collection-macros": "^7.12",
        "spatie/laravel-medialibrary": "^10.6",
        "spatie/laravel-menu": "^4.1",
        "spatie/laravel-ray": "^1.31",
        "spatie/laravel-sitemap": "^6.2",
        "spatie/schema-org": "^3.13",
        "stechstudio/laravel-php-cs-fixer": "^3.1",
        "stevebauman/location": "^6.5",
        "tcg/voyager": "^1.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "fossbarrow/laravel-phpcs": "dev-main",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "laravel/ui": "^4.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0",
        "phpmd/phpmd": "@stable",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "squizlabs/php_codesniffer": "3.*",
        "wulfheart/laravel-actions-ide-helper": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpcs": [
            "./vendor/bin/phpcs"
        ],
        "phpcbf": [
            "./vendor/bin/phpcbf"
        ],
        "phpmd": [
            "./vendor/bin/phpmd app,config,resources,routes,tests text phpmd.xml"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
